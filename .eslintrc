{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-unresolved": [
        2, 
        { "caseSensitive": false }
    ]
  }
}
